{
    "name": "nestia-template",
    "version": "0.0.0",
    "description": "nestia를 적용한 nestjs 서버",
    "main": "build/main",
    "scripts": {
        "check": "ts-patch check",
        "postinstall": "husky install && ts-patch install",
        "---------------------------------------------": "",
        "eslint": "eslint src",
        "eslint:fix": "eslint src --fix",
        "prettier": "prettier . -c",
        "prettier:fix": "prettier . -w",
        "----------------------------------------------": "",
        "sdk": "rimraf ./sdk && NODE_ENV=test nestia sdk",
        "build": "rimraf build && tsc -p ./tsconfig.build.json",
        "build:doc": "NODE_ENV=test nestia swagger",
        "build:sdk": "rimraf ./sdk && NODE_ENV=test nestia sdk && rimraf ./packages/sdk/lib && tsc -p ./tsconfig.sdk.json",
        "build:test": "npm run build:sdk && rimraf tbuild && tsc -p ./tsconfig.test.json",
        "-----------------------------------------------": "",
        "start": "npm run build && NODE_ENV=development node build/main",
        "test": "npm run build:test && NODE_ENV=test node tbuild/test",
        "doc": "npm run build:doc && node ./packages/swagger/index.mjs",
        "-------------------------------------------------": "",
        "db:sch": "ts-node prisma",
        "db:update": "rimraf db && npm run db:sch && prisma generate && find ./db/runtime -type f -exec sed -i '' 's/\\r$//' {} \\;",
        "db:sync": "npm run db:update && prisma migrate dev",
        "db:init": "prisma migrate reset --force",
        "db:deploy": "prisma migrate deploy"
    },
    "author": "industriously",
    "license": "UNLICENSED",
    "repository": {
        "type": "git",
        "url": "git@github.com-industriously:industriously/nestia-template.git"
    },
    "keywords": [
        "nestjs",
        "nestia"
    ],
    "dependencies": {
        "@fxts/core": "^0.20.0",
        "@nestia/core": "^2.0.4",
        "@nestia/fetcher": "^2.0.4",
        "@nestjs/common": "^10.2.5",
        "@nestjs/core": "^10.2.5",
        "@nestjs/platform-express": "^10.2.5",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.3.1",
        "helmet": "^7.0.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "typia": "^5.0.4",
        "winston": "^3.10.0"
    },
    "devDependencies": {
        "@nestia/e2e": "^0.3.6",
        "@nestia/sdk": "^2.0.4",
        "@prisma/client": "^5.3.1",
        "@trivago/prettier-plugin-sort-imports": "^4.2.0",
        "@types/cookie-parser": "^1.4.4",
        "@types/express": "^4.17.17",
        "@types/node": "^20.6.2",
        "@typescript-eslint/eslint-plugin": "^6.7.2",
        "@typescript-eslint/parser": "^6.7.2",
        "eslint-plugin-deprecation": "^2.0.0",
        "husky": "^8.0.3",
        "nestia": "^4.5.0",
        "prettier": "^3.0.3",
        "prisma": "^5.3.1",
        "rimraf": "^5.0.1",
        "schemix": "^1.14.1",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.1",
        "ts-patch": "^3.0.2",
        "typescript": "^5.2.2",
        "typescript-transform-paths": "^3.4.6"
    },
    "prisma": {
        "schema": "prisma/schema.prisma"
    }
}
