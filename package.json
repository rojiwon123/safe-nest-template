{
    "name": "nestia-template",
    "version": "0.0.2",
    "description": "nestia를 적용한 nestjs 서버",
    "main": "build/main",
    "scripts": {
        "--------------------execute--------------------": "",
        "start": "npm run build && NODE_ENV=development node build/main",
        "test": "npm run build:test && NODE_ENV=test node tbuild/test",
        "--------------------build--------------------": "",
        "build": "rimraf ./build && npm run lint && tsc -p ./tsconfig.build.json",
        "build:prisma": "rimraf ./prisma/client && prisma generate --schema=./prisma/schemas",
        "build:swagger": "rimraf ./packages/api/swagger/swagger.json && nestia swagger",
        "build:sdk": "rimraf ./sdk && nestia sdk && rimraf ./packages/api/lib && tsc -p ./tsconfig.sdk.json && rimraf ./sdk",
        "build:nestia": "npm run build:sdk && npm run build:swagger",
        "build:test": "rimraf ./tbuild && tsc -p ./tsconfig.test.json",
        "--------------------init--------------------": "",
        "check": "ts-patch check",
        "postinstall": "husky",
        "prepare": "ts-patch install && typia patch && npm run build:prisma",
        "prettier": "prettier . -w",
        "lint": "eslint ./src",
        "--------------------db--------------------": "",
        "db:sync": "npm run build:prisma && prisma migrate dev --name init --schema=./prisma/schemas",
        "db:init": "prisma migrate reset --force",
        "db:deploy": "prisma migrate deploy --schema=./prisma/schemas"
    },
    "author": "rojiwon123",
    "license": "UNLICENSED",
    "repository": {
        "type": "git",
        "url": "https://github.com/rojiwon123/nestia-template.git"
    },
    "keywords": [
        "nestjs",
        "nestia"
    ],
    "dependencies": {
        "@aws-sdk/client-cloudwatch-logs": "^3.616.0",
        "@fxts/core": "^1.0.1",
        "@nestia/core": "^3.7.2",
        "@nestjs/common": "^10.3.10",
        "@nestjs/core": "^10.3.10",
        "@nestjs/platform-express": "^10.3.10",
        "@rojiwon123/fetch": "^1.0.3",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.4.5",
        "helmet": "^7.1.0",
        "rxjs": "^7.8.1",
        "typia": "^6.5.2",
        "winston": "^3.13.1"
    },
    "devDependencies": {
        "@nestia/e2e": "^0.7.0",
        "@nestia/sdk": "^3.7.2",
        "@project/api": "file:packages/api",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/cookie-parser": "^1.4.7",
        "@types/express": "^4.17.21",
        "@types/node": "^20.14.11",
        "@typescript-eslint/eslint-plugin": "^7.16.1",
        "@typescript-eslint/parser": "^7.16.1",
        "eslint-plugin-deprecation": "^3.0.0",
        "husky": "^9.1.1",
        "nestia": "^5.4.0",
        "prettier": "^3.3.3",
        "prisma": "^5.17.0",
        "prisma-markdown": "^1.0.9",
        "rimraf": "^6.0.1",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.2",
        "ts-patch": "^3.2.1",
        "typescript": "^5.5.3",
        "typescript-transform-paths": "^3.4.7"
    }
}
