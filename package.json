{
    "name": "nestia-template",
    "version": "0.0.2",
    "description": "nestia를 적용한 nestjs 서버",
    "main": "build/main",
    "scripts": {
        "--------------------execute--------------------": "",
        "start": "npm run build && NODE_ENV=development node build/main",
        "test": "npm run build:sdk && npm run build:test && NODE_ENV=test node tbuild/test",
        "--------------------build--------------------": "",
        "build": "npm run lint && rimraf ./build && tsc -p ./tsconfig.build.json",
        "build:test": "rimraf ./tbuild && tsc -p ./tsconfig.test.json",
        "build:prisma": "rimraf ./prisma/client && prisma generate --schema=./prisma/schemas",
        "build:swagger": "rimraf ./packages/api/swagger/openapi.json && nestia swagger",
        "build:sdk": "rimraf ./sdk && nestia sdk && rimraf ./packages/api/lib && tsc -p ./tsconfig.sdk.json && rimraf ./sdk",
        "build:nestia": "npm run build:sdk && npm run build:swagger",
        "--------------------init--------------------": "",
        "check": "ts-patch check",
        "prepare": "ts-patch install && typia patch && npm run build:prisma",
        "postinstall": "[ -f ./setup-hook.sh ] && /bin/sh ./setup-hook.sh || true",
        "prettier": "prettier . -w",
        "lint": "npx eslint .",
        "--------------------db--------------------": "",
        "db:sync": "prisma migrate dev --schema=./prisma/schemas",
        "db:deploy": "prisma migrate deploy --schema=./prisma/schemas",
        "db:console": "prisma studio --schema=./prisma/schemas"
    },
    "author": "rojiwon123",
    "license": "UNLICENSED",
    "repository": {
        "type": "git",
        "url": "https://github.com/rojiwon123/nestia-template.git"
    },
    "keywords": [
        "nestjs",
        "nestia"
    ],
    "dependencies": {
        "@aws-sdk/client-cloudwatch-logs": "^3.669.0",
        "@fxts/core": "^1.0.1",
        "@nestia/core": "^3.17.0",
        "@nestjs/common": "^10.4.4",
        "@nestjs/core": "^10.4.4",
        "@nestjs/platform-express": "^10.4.4",
        "@prisma/client": "^5.20.0",
        "cookie-parser": "^1.4.7",
        "dotenv": "^16.4.5",
        "google-auth-library": "^9.14.2",
        "helmet": "^8.0.0",
        "rxjs": "^7.8.1",
        "typia": "^6.11.2",
        "winston": "^3.15.0"
    },
    "devDependencies": {
        "@eslint/compat": "^1.1.1",
        "@nestia/e2e": "^0.7.0",
        "@nestia/sdk": "^3.17.0",
        "@project/api": "file:packages/api",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/cookie-parser": "^1.4.7",
        "@types/express": "^5.0.0",
        "@types/node": "^22.7.5",
        "eslint-plugin-deprecation": "^3.0.0",
        "nestia": "^6.2.0",
        "nodemon": "^3.1.7",
        "prettier": "^3.3.3",
        "prisma": "^5.20.0",
        "prisma-markdown": "^1.0.9",
        "rimraf": "^6.0.1",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.2",
        "ts-patch": "^3.2.1",
        "typescript": "5.5.4",
        "typescript-eslint": "^8.8.1",
        "typescript-transform-paths": "^3.5.1"
    }
}
