{
    "name": "nestia-template",
    "version": "0.0.0",
    "description": "nestia를 적용한 nestjs 서버",
    "main": "build/main",
    "scripts": {
        "--------------------execute--------------------": "",
        "start": "npm run build && NODE_ENV=development node build/main",
        "test": "npm run build:test && NODE_ENV=test node tbuild/test",
        "swagger": "npm run build:swagger && node ./packages/api/swagger/index.cjs",
        "--------------------build--------------------": "",
        "build": "rimraf ./build && tsc -p ./tsconfig.build.json",
        "build:prisma": "rimraf ./prisma/client && prisma generate",
        "build:swagger": "rimraf ./packages/api/swagger/swagger.json && NODE_ENV=test nestia swagger",
        "build:sdk": "rimraf ./sdk && NODE_ENV=test nestia sdk && rimraf ./packages/sdk/lib && tsc -p ./tsconfig.sdk.json && rimraf ./sdk",
        "build:nestia": "npm run build:sdk && npm run build:swagger",
        "build:test": "rimraf ./tbuild && tsc -p ./tsconfig.test.json",
        "--------------------init--------------------": "",
        "check": "ts-patch check",
        "postinstall": "husky install",
        "prepare": "ts-patch install && typia patch",
        "prettier": "prettier . -w",
        "--------------------db--------------------": "",
        "db:sync": "npm run build:prisma && prisma migrate dev --name init",
        "db:init": "prisma migrate reset --force",
        "db:deploy": "prisma migrate deploy"
    },
    "author": "rojiwon123",
    "license": "UNLICENSED",
    "repository": {
        "type": "git",
        "url": "https://github.com/rojiwon123/nestia-template.git"
    },
    "keywords": [
        "nestjs",
        "nestia"
    ],
    "dependencies": {
        "@aws-sdk/client-cloudwatch-logs": "^3.583.0",
        "@fxts/core": "^0.29.0",
        "@nestia/core": "^3.2.0",
        "@nestjs/common": "^10.3.8",
        "@nestjs/core": "^10.3.8",
        "@nestjs/platform-express": "^10.3.8",
        "@rojiwon123/fetch": "^0.2.0",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.4.5",
        "helmet": "^7.1.0",
        "rxjs": "^7.8.1",
        "typia": "^6.0.3",
        "winston": "^3.13.0"
    },
    "devDependencies": {
        "@nestia/e2e": "^0.4.3",
        "@nestia/sdk": "^3.2.0",
        "@project/api": "file:packages/api",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/cookie-parser": "^1.4.7",
        "@types/express": "^4.17.21",
        "@types/node": "^20.12.12",
        "@typescript-eslint/eslint-plugin": "^7.10.0",
        "@typescript-eslint/parser": "^7.10.0",
        "eslint-plugin-deprecation": "^2.0.0",
        "husky": "^9.0.11",
        "nestia": "^5.3.0",
        "prettier": "^3.2.5",
        "prisma": "^5.14.0",
        "prisma-markdown": "^1.0.9",
        "rimraf": "^5.0.7",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.2",
        "ts-patch": "^3.1.2",
        "typescript": "^5.4.5",
        "typescript-transform-paths": "^3.4.7"
    }
}
